{
  "name": "captain-spike",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --reporters jest-dot-reporter",
    "test:ci": "jest --reporters jest-silent-reporter --coverage",
    "test:watch": "npm run test -- --watch",
    "prebuild": "rimraf dist/",
    "build": "babel ./src/ --out-dir dist/ --copy-files",
    "prestart": "npm run build",
    "start": "node bin/cli.js",
    "lint": "eslint src/**/*.js features/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:ci": "eslint -o reports/eslint.json -f json src/**/*.js features/**/*.js",
    "preacceptance-test": "npm run build && rimraf dist-acceptance-test/ && babel ./features/ --out-dir dist-acceptance-test/ --copy-files",
    "acceptance-test": "cucumber-js -f node_modules/cucumber-pretty --require dist-acceptance-test",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-preset-es2015": "^6.24.1",
    "cucumber": "^4.2.1",
    "cucumber-pretty": "^1.4.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "faker": "^4.1.0",
    "jest": "^23.4.1",
    "jest-dot-reporter": "^1.0.6",
    "jest-silent-reporter": "0.0.5",
    "rimraf": "^2.6.2",
    "sonar-scanner": "^3.1.0",
    "tmp": "0.0.33",
    "touch": "^3.1.0",
    "unique-filename": "^1.1.0"
  },
  "jest": {
    "transform": {}
  },
  "dependencies": {
    "simple-git": "^1.96.0"
  }
}
